{
  "name": "ra-data-simple-prisma",
  "version": "0.3.2",
  "description": "Create a fullstack react-admin app adding just one file",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --dts",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts",
    "lint": "TIMING=1 eslint src --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "prepublish": "pnpm build",
    "test": "jest"
  },
  "files": [
    "/dist"
  ],
  "keywords": [
    "prisma",
    "rest",
    "marmelab",
    "react-admin",
    "dataprovider"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/codeledge/ra-prisma-nextjs.git",
    "directory": "packages/ra-data-simple-prisma"
  },
  "author": "Orlando Groppo <orlando.groppo@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^3.14.0",
    "@types/node": "^17.0.32",
    "axios": "^0.27.2",
    "set-value": "^4.1.0"
  },
  "devDependencies": {
    "jest": "^28.1.0",
    "react-admin": "^4.0.2",
    "tsup": "^5.12.7",
    "typescript": "^4.6.4"
  }
}
